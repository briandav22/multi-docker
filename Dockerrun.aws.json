{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions": [
        {
            "name":"client",
            "image":"briandav22/multi-client",
            "hostname":"client",
            "essential":false
        },
        {
            "name":"server",
            "image":"briandav22/multi-server",
            "hostname":"api",
            "essential":false
        },
        {
            "name":"worker",
            "image":"briandav22/multi-worker",
            "hostname":"worker",
            "essential":false
        },
        {
            "name":"nginx",
            "image":"briandav22/multi-nginx",
            "hostname":"nginx",
            "essential":true,
            "portMappings":[
                {"hostPort":80,
                "containerPort":80}
            ],
            "links":["client","server"]
        }

    ]
}